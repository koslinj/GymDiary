{
  "expo": {
    "backgroundColor": "#0F172A",
    "name": "GymDiary",
    "slug": "gymdiary",
    "version": "1.3.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gymdiary",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0F172A"
      },
      "package": "com.koslinj.gymdiary",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAUtcTuc9WSR7J2Sfq85U5Z-S90Ozhe4MU"
        }
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "react-native-localization-settings",
        {
          "languages": [
            "en",
            "pl"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "213157dc-64d9-4949-b3bd-41a9c0721906"
      }
    }
  }
}